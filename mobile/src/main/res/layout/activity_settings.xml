<?xml version="1.0" encoding="utf-8"?>
<android.support.design.widget.CoordinatorLayout android:id="@+id/coordinatorLayout"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:animateLayoutChanges="true"
    android:background="@color/map_background">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="goBack"
            android:orientation="horizontal"
            android:paddingTop="16dp">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingBottom="4dp"
                android:paddingLeft="19dp"
                android:paddingRight="19dp"
                android:paddingTop="4dp"
                android:src="@mipmap/shape_10_copy_13_2_copy_3" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:fontFamily="sans-serif"
                android:gravity="center_horizontal"
                android:lineSpacingExtra="14.8sp"
                android:text="Settings"
                android:textColor="#ffffff"
                android:textSize="24sp"
                android:textStyle="normal"
                tools:text="Settings" />

        </FrameLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical">

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/divider_color" />

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:paddingBottom="20.3dp"
                android:paddingRight="28dp"
                android:paddingTop="17dp">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:paddingLeft="28dp"
                    android:src="@mipmap/layer_5" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:fontFamily="sans-serif"
                    android:lineSpacingExtra="10.3sp"
                    android:paddingLeft="81.3dp"
                    android:text="Ride History"
                    android:textColor="#E094B1"
                    android:textSize="20sp"
                    android:textStyle="normal" />

            </FrameLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/divider_color" />

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:paddingBottom="20.3dp"
                android:paddingRight="28dp"
                android:paddingTop="17dp">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:paddingLeft="28dp"
                    android:src="@mipmap/layer_12" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="sans-serif"
                    android:lineSpacingExtra="10.3sp"
                    android:paddingLeft="81.3dp"
                    android:text="My Tokens"
                    android:textColor="#E4AAA5"
                    android:textSize="20sp"
                    android:textStyle="normal" />

            </FrameLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/divider_color" />

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:paddingBottom="20.3dp"
                android:paddingRight="28dp"
                android:paddingTop="17dp">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:paddingLeft="28dp"
                    android:src="@mipmap/layer_10" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="sans-serif"
                    android:lineSpacingExtra="10.3sp"
                    android:paddingLeft="81.3dp"
                    android:text="Rinkeby"
                    android:textColor="#E4AAA5"
                    android:textSize="20sp"
                    android:textStyle="normal" />

                <ImageView
                    android:id="@+id/chooseNetwork"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical|right"
                    android:src="@mipmap/shape_10_copy_13_2_copy_4" />

            </FrameLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/divider_color" />

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:paddingBottom="20.3dp"
                android:paddingRight="28dp"
                android:paddingTop="17dp">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:paddingLeft="28dp"
                    android:src="@mipmap/ellipse_1" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="sans-serif"
                    android:lineSpacingExtra="10.3sp"
                    android:paddingLeft="81.3dp"
                    android:text="Dummy Data"
                    android:textColor="#E7B79E"
                    android:textSize="20sp"
                    android:textStyle="normal" />

                <com.suke.widget.SwitchButton
                    android:id="@+id/switch_button"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical|right"
                    app:sb_checked="false"
                    app:sb_checked_color="#27B894"
                    app:sb_show_indicator="false"
                    app:sb_uncheckcircle_color="#40536C" />


            </FrameLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/divider_color" />

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:paddingBottom="20.3dp"
                android:paddingRight="28dp"
                android:paddingTop="17dp">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:paddingLeft="28dp"
                    android:src="@mipmap/layer_7" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="sans-serif"
                    android:lineSpacingExtra="10.3sp"
                    android:paddingLeft="81.3dp"
                    android:text="Leader Board"
                    android:textColor="#E7B79E"
                    android:textSize="20sp"
                    android:textStyle="normal" />

            </FrameLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/divider_color" />

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:paddingBottom="20.3dp"
                android:paddingRight="28dp"
                android:paddingTop="17dp">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:paddingLeft="28dp"
                    android:src="@mipmap/layer_8" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="sans-serif"
                    android:lineSpacingExtra="10.3sp"
                    android:paddingLeft="81.3dp"
                    android:text="Profile"
                    android:textColor="#E8BE9B"
                    android:textSize="20sp"
                    android:textStyle="normal" />

            </FrameLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/divider_color" />

        </LinearLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="sans-serif"
            android:gravity="center"
            android:lineSpacingExtra="@dimen/wallet_text_extra_spacing"
            android:longClickable="false"
            android:onClick="onAddressClick"
            android:paddingBottom="@dimen/wallet_text_padding_bottom"
            android:paddingLeft="16dp"
            android:paddingRight="16dp"
            android:paddingTop="16dp"
            android:text="0x5c5699e507463E1A234d417B7CA3399aC93bC8cF"
            android:textColor="@color/white_opacity_18"
            android:textSize="@dimen/wallet_text_text_size"
            android:textStyle="normal"
            android:transitionName="wallet"
            tools:text="0x5c5699e507463E1A234d417B7CA3399aC93bC8cF" />

    </LinearLayout>
</android.support.design.widget.CoordinatorLayout>